export type ICommerce = {
  id: number;
  rutcommerce: string;
  commerceregister: string;
  tipo: string;
  gravedad: string;
  calle: string;
  numeracion: string;
  descripcion: string;
  fecha: string; 
};
export type GeoLocation = {
  id: number; // útil si guardás ubicaciones
  latitude: number;
  longitude: number;
};

export type MediaItem = {
  id: number; // si querés trackearlo también
  uri: string;
  type: 'photo' | 'video';
};

export type ModalOptions = string[];

export type FooterActions = {
  
  onCancel: () => void;
  onClear: () => void;
  onSave: () => void;
};

export type MediaSourceType = 'photo' | 'video';
export type MediaOrigin = 'camera' | 'gallery';
